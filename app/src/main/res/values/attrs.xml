<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TasksCompletedView">
        <attr name="radius" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="roundWidth" format="dimension" />
        <attr name="circleColor" format="color" />
        <attr name="ringColor" format="color" />
        <attr name="roundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="IconicsImageView">
        <attr name="iiv_icon" format="string" />
        <attr name="iiv_color" format="color|reference" />
        <attr name="iiv_size" format="dimension|reference" />
        <attr name="iiv_padding" format="dimension|reference" />
        <attr name="iiv_contour_color" format="color|reference" />
        <attr name="iiv_contour_width" format="dimension|reference" />
        <attr name="iiv_background_color" format="color|reference" />
        <attr name="iiv_corner_radius" format="dimension|reference" />
    </declare-styleable>
    <declare-styleable name="Iconics">
        <attr name="ico_icon" format="string" />
        <attr name="ico_color" format="color|reference" />
        <attr name="ico_size" format="dimension|reference" />
        <attr name="ico_offset_x" format="dimension|reference" />
        <attr name="ico_offset_y" format="dimension|reference" />
        <attr name="ico_padding" format="dimension|reference" />
        <attr name="ico_contour_color" format="color|reference" />
        <attr name="ico_contour_width" format="dimension|reference" />
        <attr name="ico_background_color" format="color|reference" />
        <attr name="ico_corner_radius" format="dimension|reference" />
    </declare-styleable>
</resources>
